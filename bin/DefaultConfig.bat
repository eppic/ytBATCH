::Name pattern of downloaded files
set FileNaming=/%%(title)s.%%(ext)s
::Destination of downloaded files
set Destination=%userprofile%\Downloads\ytBATCH
::Open file destination after download
set DestOpen=F
::Use Cookies automatically when downloading
set AutoCookies=F
::Use a default format for Video and Audio Downloads (Customizable)
set DefaultAudio=none
set DefaultVideo=none